<?xml version="1.0" encoding="UTF-8"?><ui version="4.0"> <class>ModelManageDialog</class> <widget class="QDialog" name="ModelManageDialog">   <property name="font"><font><pointsize>12</pointsize></font></property>  <property name="windowTitle"><string>波管型号管理</string></property>  <layout class="QVBoxLayout" name="verticalLayout">    <!-- 最外层垂直布局: 第一行放主内容，第二行放按钮框 -->    <item>      <!-- 主内容区域：左右分割 -->      <layout class="QHBoxLayout" name="hLayoutMain">        <!-- 左侧：型号列表组 -->        <item>          <widget class="QGroupBox" name="groupModels">            <property name="title"><string>型号列表</string></property>            <layout class="QVBoxLayout" name="vLayoutModels">              <!-- 按钮行 -->              <item>                <layout class="QHBoxLayout" name="hLayoutModelBtns">                  <item>                    <widget class="QPushButton" name="btnAddModel">                      <property name="text"><string>＋型号</string></property>                    </widget>                  </item>                  <item>                    <widget class="QPushButton" name="btnRemoveModel">                      <property name="text"><string>－型号</string></property>                    </widget>                  </item>                </layout>              </item>              <!-- 列表行 -->              <item>                <widget class="QListWidget" name="listModels"/>              </item>            </layout>          </widget>        </item>        <!-- 右侧：参数组 -->        <item>          <widget class="QGroupBox" name="groupParams">            <property name="title"><string>参数列表</string></property>            <layout class="QVBoxLayout" name="vLayoutParams">              <!-- 按钮行 -->              <item>                <layout class="QHBoxLayout" name="hLayoutParamBtns">                  <item>                    <widget class="QPushButton" name="btnAddParam">                      <property name="text"><string>＋参数</string></property>                    </widget>                  </item>                  <item>                    <widget class="QPushButton" name="btnRemoveParam">                      <property name="text"><string>－参数</string></property>                    </widget>                  </item>                </layout>              </item>              <!-- 表单行 -->             <item>              <widget class="QListWidget" name="listParams">                <property name="alternatingRowColors"><bool>true</bool></property>                <property name="selectionMode"><enum>QAbstractItemView::SingleSelection</enum></property>              </widget>            </item>            </layout>          </widget>        </item>      </layout>    </item>    <!-- 底部：保存/取消按钮框 -->    <item>      <widget class="QDialogButtonBox" name="buttonBox">        <property name="standardButtons">          <set>QDialogButtonBox::Save|QDialogButtonBox::Cancel</set>        </property>      </widget>    </item>  </layout> </widget> <resources/> <connections/></ui>